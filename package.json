{
  "private": true,
  "name": "test-preact-web-adapter",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "preact watch",
    "build": "preact build --no-prerender",
    "build-serve": "sirv build --port 8080 --cors --single",
